<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>MaServlet</servlet-name>
  <servlet-class>dta.MaServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>PizzaServletWebApi</servlet-name>
  <servlet-class>web.PizzaServletWebApi</servlet-class>
  </servlet>
    <servlet>
  <servlet-name>PizzaServletWeb</servlet-name>
  <servlet-class>web.PizzaServletWeb</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>PizzaServletAjoutWeb</servlet-name>
  <servlet-class>web.PizzaServletAjoutWeb</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>PizzaServletSuprimer</servlet-name>
  <servlet-class>web.PizzaServletSuprimer</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>PizzaServletUpdate</servlet-name>
  <servlet-class>web.PizzaServletUpdate</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>MaServlet</servlet-name>
  <url-pattern>/maservlet</url-pattern>
  
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>PizzaServletUpdate</servlet-name>
  <url-pattern>/update</url-pattern>
  
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>PizzaServletWebApi</servlet-name>
  <url-pattern>/listes</url-pattern>
  
  </servlet-mapping>
  
   <servlet-mapping>
  <servlet-name>PizzaServletWeb</servlet-name>
  <url-pattern>/pizzawebapp</url-pattern>
  
  </servlet-mapping>
  
   <servlet-mapping>
  <servlet-name>PizzaServletAjoutWeb</servlet-name>
  <url-pattern>/ajoutpizza</url-pattern>
  
  </servlet-mapping>
  
    <servlet-mapping>
  <servlet-name>PizzaServletSuprimer</servlet-name>
  <url-pattern>/delete</url-pattern>
  
  </servlet-mapping>
</web-app>
